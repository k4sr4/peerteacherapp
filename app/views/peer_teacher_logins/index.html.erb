<p id="notice"><%= notice %></p>

<h1>Listing Peer Teacher Logins</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @peer_teacher_logins.each do |peer_teacher_login| %>
      <tr>
        <td><%= peer_teacher_login.first_name %></td>
        <td><%= peer_teacher_login.last_name %></td>
        <td><%= link_to 'Show', peer_teacher_login %></td>
        <td><%= link_to 'Edit', edit_peer_teacher_login_path(peer_teacher_login) %></td>
        <td><%= link_to 'Destroy', peer_teacher_login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Peer teacher login', new_peer_teacher_login_path %>
